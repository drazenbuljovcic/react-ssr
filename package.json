{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "React SSR app with express server and webpack bundling.",
  "main": "./server/server.js",
  "scripts": {
    "watch": "node_modules/.bin/webpack --progress --colors --watch",
    "build": "rm -rf dist/ && NODE_ENV=production webpack -p --progress --colors",
    "start": "npm run build && babel-node server/server.js | node",
    "start:dev": "node_modules/.bin/nodemon server/server.js --exec babel-node --ext pug,js,css"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "dependencies": {
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "webpack": "^1.12.13",
    "autoprefixer": "^6.6.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.1",
    "pug": "^2.0.0-beta6",
    "reload": "^1.1.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zendra1994/react-ssr.git"
  },
  "author": "Drazen Buljovcic <drazen.buljovcic@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zendra1994/react-ssr/issues"
  },
  "homepage": "https://github.com/zendra1994/react-ssr#readme"
}
